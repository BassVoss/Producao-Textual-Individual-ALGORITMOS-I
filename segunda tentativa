//ainda não consegui fazer com que leia as três opções//

console.log ("Qual combubustível é a melhor opção para abastecer?")

var gasolina=prompt ("Preço da gasolina:")

var alcool=prompt ("Preço do álcool:")

total=alcool/gasolina
console.log (alcool/gasolina)

if (total<0.69){console.log("É melhor abastecer com álcool")
               }else if (total==0.70){console.log("Ambos os combustíveis são vantajosos")
                                     }else{console.log("É melhor abastecer com gasolina")}
    
  
